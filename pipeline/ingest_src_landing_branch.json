{
	"name": "ingest_src_landing_branch",
	"properties": {
		"activities": [
			{
				"name": "Copy Branch Data from Source to Landing",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "branch_id",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "branch_id",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "branch_country",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "branch_country",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "branch_city",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "branch_city",
									"type": "String",
									"physicalType": "String"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DS_SQL_GENERIC",
						"type": "DatasetReference",
						"parameters": {
							"SchemaName": "dbo",
							"TableName": "Branch"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DS_Branch_Landing",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Policy Project"
		},
		"annotations": []
	}
}