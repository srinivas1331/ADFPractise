{
	"name": "insgest_thirdparty_to_landing_ecommerce",
	"properties": {
		"activities": [
			{
				"name": "Copy Thirdparty to Laninding Ecommerce",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_Sales_Ecom_Thirdparty",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_SALES_LANDING_CSV",
						"type": "DatasetReference",
						"parameters": {
							"folderpath": "ECOM/@{formatDateTime(utcNow(),'yyyy-MM-dd')}"
						}
					}
				]
			}
		],
		"folder": {
			"name": "Sales Project"
		},
		"annotations": []
	}
}