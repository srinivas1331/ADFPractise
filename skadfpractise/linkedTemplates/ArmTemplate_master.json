{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "skadfpractise"
		},
		"AzureFileStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureFileStorage1'"
		},
		"LS_ADLS_Generic_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_Generic'"
		},
		"LS_LandingStg_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_LandingStg'"
		},
		"LS_SKALAVAADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_SKALAVAADLS'"
		},
		"LS_SKSAMPLE_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SKSAMPLE'"
		},
		"LS_SQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQLDB'"
		},
		"LS_SQLDB_Key_Vault_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQLDB_Key_Vault'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=azserver123.database.windows.net;Initial Catalog=ADFPractise;User ID=azure123"
		},
		"LS_SQL_Generic_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_Generic'"
		},
		"LS_ADLS_Generic_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://skalavaadls.dfs.core.windows.net/"
		},
		"LS_KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KVskalava.vault.azure.net/"
		},
		"LS_LandingStg_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://landingstg.dfs.core.windows.net/"
		},
		"LS_SKALAVAADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://skalavaadls.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "skadfpractise_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureFileStorage1_connectionString": {
						"value": "[parameters('AzureFileStorage1_connectionString')]"
					},
					"LS_ADLS_Generic_accountKey": {
						"value": "[parameters('LS_ADLS_Generic_accountKey')]"
					},
					"LS_LandingStg_accountKey": {
						"value": "[parameters('LS_LandingStg_accountKey')]"
					},
					"LS_SKALAVAADLS_accountKey": {
						"value": "[parameters('LS_SKALAVAADLS_accountKey')]"
					},
					"LS_SKSAMPLE_connectionString": {
						"value": "[parameters('LS_SKSAMPLE_connectionString')]"
					},
					"LS_SQLDB_connectionString": {
						"value": "[parameters('LS_SQLDB_connectionString')]"
					},
					"LS_SQL_Generic_connectionString": {
						"value": "[parameters('LS_SQL_Generic_connectionString')]"
					},
					"LS_ADLS_Generic_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_Generic_properties_typeProperties_url')]"
					},
					"LS_KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_LandingStg_properties_typeProperties_url": {
						"value": "[parameters('LS_LandingStg_properties_typeProperties_url')]"
					},
					"LS_SKALAVAADLS_properties_typeProperties_url": {
						"value": "[parameters('LS_SKALAVAADLS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "skadfpractise_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_SQLDB_Key_Vault_connectionString": {
						"value": "[parameters('LS_SQLDB_Key_Vault_connectionString')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'skadfpractise_ArmTemplate_0')]"
			]
		},
		{
			"name": "skadfpractise_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'skadfpractise_ArmTemplate_1')]"
			]
		}
	]
}